#include <iostream>
using namespace std;

void printArray(int arr[], int size)

{
  cout << " in print array" << endl;
  cout << " ";
  for (int i = 0; i < size; i++) {
    cout << arr[i] << " ";
  }
  cout << endl;

  cout << " out print array" << endl;
}

